{
  "name": "react-ts-universal-boilerplate",
  "version": "0.1.0",
  "description":
    "A React Universal boierplate using TypeScript, Express, Redux and Webpack bundler",
  "main": "src/index.ts",
  "repository":
    "git@github.com:nhanhoangtrong/kendy-react-ts-universal-boilerplate.git",
  "author": "Nhan Hoang <siege250294@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/server",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build":
      "cross-env NODE_ENV=production && rimraf dist/ && webpack --config webpack/client.config.js && webpack --config webpack/server.config.js ",
    "build:client":
      "cross-env NODE_ENV=production webpack --config webpack/client.config.js",
    "build:server":
      "cross-env NODE_ENV=production webpack --config webpack/server.config.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test":
      "mocha -r ts-node/register -r ignore-styles -r jsdom-global/register test/*.ts"
  },
